{
    // WIP
    "typescript.suggest.paths": false,
    "javascript.suggest.paths": false,
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.suggestSelection": "recentlyUsed",
      "editor.suggest.showKeywords": false
    },
    "javascript.suggest.completeFunctionCalls": true,
    "typescript.suggest.completeFunctionCalls": true,
    "go.useLanguageServer": true,
    "[go]": {
      "editor.insertSpaces": true,
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "golang.go"
    },
    // "js/ts.implicitProjectConfig.checkJs": true,
    "editor.suggest.localityBonus": true,
    "editor.renderWhitespace": "none",
    "files.exclude": {
      // "node_modules": true,
      // "package-lock.json": true
    },
    "search.exclude": {
      "**/tsconfig.tsbuildinfo": true,
      "**/pnpm-lock.yaml": true,
      "**/.build": true,
      "**/.gh-pages": true,
      "**/.next": true,
      "**/bower_components": true,
      "**/build": true,
      "**/coverage": true,
      "**/dist": true
    },
    // Defaults
    "editor.fontLigatures": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.wordBasedSuggestions": "off",
    "editor.smoothScrolling": true,
    "editor.cursorBlinking": "phase",
    "files.trimTrailingWhitespace": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "files.autoSave": "onWindowChange",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.addMissingImports": "explicit",
      // "source.organizeImports": "explicit",
      "source.fixAll.eslint": "explicit"
    },
    // Hiding Stuff
    "editor.snippetSuggestions": "inline",
    // "editor.lineNumbers": "off",
    "workbench.startupEditor": "none",
    "editor.renderLineHighlight": "none",
    "editor.renderControlCharacters": false,
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    "editor.occurrencesHighlight": "off",
    "workbench.editor.enablePreview": false,
    "editor.hover.enabled": true,
    "editor.matchBrackets": "never",
    "workbench.tips.enabled": false,
    "editor.lightbulb.enabled": "off",
    "editor.guides.indentation": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.fontFamily": "Dank Mono, FiraCode NF, JetBrains Mono, MonoLisa",
    "editor.fontSize": 15,
    "editor.lineHeight": 30,
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontFamily": "Dank Mono, FiraCode NF, JetBrains Mono, MonoLisa",
    "editor.wordWrapColumn": 90,
    "terminal.integrated.lineHeight": 1.2,
    "editor.wordWrap": "on",
    "editor.formatOnSave": true,
    "files.eol": "\n",
    // Search
    "search.useIgnoreFiles": false,
    // Prettier
    "prettier.arrowParens": "always",
    "prettier.printWidth": 80,
    "prettier.semi": false,
    "prettier.tabWidth": 2,
    "prettier.useEditorConfig": false,
    "prettier.requireConfig": true,
    "prettier.singleQuote": true,
    "prettier.bracketSpacing": true,
    "prettier.endOfLine": "lf",
    "prettier.htmlWhitespaceSensitivity": "css",
    "prettier.proseWrap": "always",
    "prettier.quoteProps": "consistent",
    "prettier.trailingComma": "all",
    // Window
    "zenMode.hideActivityBar": false,
    "window.titleBarStyle": "custom",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "window.density.editorTabHeight": "compact",
    // Vim stuff
    /*
      "vim.statusBarColorControl": true,
      "vim.statusBarColors.normal": ["#8FBCBB", "#434C5E"],
      "vim.statusBarColors.insert": "#BF616A",
      "vim.statusBarColors.visual": "#B48EAD",
      "vim.statusBarColors.visualline": "#B48EAD",
      "vim.statusBarColors.visualblock": "#A3BE8C",
      "vim.statusBarColors.replace": "#D08770",
      "vim.statusBarColors.commandlineinprogress": "#007ACC",
      "vim.statusBarColors.searchinprogressmode": "#007ACC",
      "vim.statusBarColors.easymotionmode": "#007ACC",
      "vim.statusBarColors.easymotioninputmode": "#007ACC",
      "vim.statusBarColors.surroundinputmode": "#007ACC",
      */
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.matchpairs": "(:),{:},[:]",
    // "vim.sneak": true,
    "vim.smartRelativeLine": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": false,
    "vim.leader": "<space>",
    "vim.foldfix": true,
    "vim.insertModeKeyBindings": [
      // {
      //     "before": ["j", "f"],
      //     "after": ["<Esc>"]
      // },
      {
        "before": ["C-h"],
        "after": ["<Left>"]
      },
      {
        "before": ["C-l"],
        "after": ["<Right>"]
      },
      {
        "before": ["C-j"],
        "after": ["<Down>"]
      },
      {
        "before": ["C-k"],
        "after": ["<Up>"]
      }
    ],
    "vim.normalModeKeyBindings": [
      // {
      //   "before": [ "<tab>"],
      //   "commands": ["workbench.action.nextEditor"]
      // },
      // {
      //   "before": ["<S-tab>"],
      //   "commands": ["workbench.action.previousEditor"]
      // },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "before": ["H"],
        "after": ["^"]
      },
      {
        "before": ["L"],
        "after": ["$"]
      },
      // {
      //     "before": ["leader", "w"],
      //     "after": ["leader", "leader", "w"],
      // },
      // {
      //     "before": ["leader", "b"],
      //     "after": ["leader", "leader", "b"],
      // },
      {
        "before": ["leader", "j"],
        "after": ["leader", "leader", "j"]
      },
      {
        "before": ["leader", "k"],
        "after": ["leader", "leader", "k"]
      }
    ],
    "vim.operatorPendingModeKeyBindings": [
      {
        "before": ["L"],
        "after": ["$"]
      },
      {
        "before": ["H"],
        "after": ["^"]
      }
    ],
    "vim.camelCaseMotion.enable": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    // "typescript.tsdk": "node_modules/typescript/lib",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "workbench.editor.showTabs": "single",
    "git.autofetch": true,
    "explorer.confirmDragAndDrop": false,
    "[html]": {
      "editor.defaultFormatter": "vscode.html-language-features"
    },
    "explorer.confirmDelete": false,
    "editor.linkedEditing": true,
    "editor.unicodeHighlight.allowedCharacters": {
      "â€™": true
    },
    "cSpell.userWords": [
      "Ansari",
      "APIKEY",
      "Arringo",
      "Arritech",
